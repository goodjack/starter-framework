{
  "name": "milkmeowo/starter-framework",
  "type": "framework",
  "description": "The Laravel / Lumen Starter Framework.",
  "keywords": ["starter-framework"],
  "license": "MIT",
  "authors": [
    {
      "name": "Milkmeowo",
      "email": "milkmeowo@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "barryvdh/laravel-cors": "^0.8.6",
    "barryvdh/laravel-ide-helper": "^2.3",
    "dingo/api": "1.0.x@dev",
    "doctrine/dbal": "^2.5",
    "dusterio/lumen-passport": "^0.1.9",
    "itsgoingd/clockwork": "^1.13",
    "league/fractal": "^0.15.0",
    "overtrue/wechat": "^3.2",
    "prettus/l5-repository": "^2.6"
  },
  "require-dev": {
    "laravel/framework": "5.4.*",
    "laravel/lumen-framework": "5.4.*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Milkmeowo\\Framework\\": "src/Framework"
    },
    "files": [
      "src/Framework/Base/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  }
}
