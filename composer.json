{
  "name": "milkmeowo/starter-framework",
  "type": "framework",
  "description": "The Laravel / Lumen Starter Framework.",
  "keywords": ["starter-framework"],
  "license": "MIT",
  "authors": [
    {
      "name": "Milkmeowo",
      "email": "milkmeowo@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "dingo/api": "1.0.x@dev",
    "dusterio/lumen-passport": "^0.1.7",
    "league/fractal": "^0.14.0",
    "prettus/l5-repository": "^2.6"
  },
  "require-dev": {
    "laravel/framework": "^5.3",
    "laravel/lumen-framework": "5.3.*",
    "phpunit/phpunit": "~5.4"
  },
  "autoload": {
    "psr-4": {
      "Milkmeowo\\Framework\\": "src/Framework"
    },
    "files": [
      "src/Framework/Base/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
